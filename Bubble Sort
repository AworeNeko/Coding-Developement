#include <iostream>
using namespace ::std;
const int Size=20;
int num[Size];
int n;



int main(){
    cout<<"Enter amount of numbers:";
    cin>>n;
    for(int i=0;i<n;i++){
        cout<<"number:"<<i+1<<":";
        cin>>num[i];
    }
   
    int temp;
    bool swap=false;
for (int j= 0; count < (n - 1); j++)
{
if (num[j] > num[j + 1])
{
temp =num[j];
num[j] =num[j + 1];
num[count + 1] = temp;

    }
}
 for (int count = 0; count < n; count++)
cout << num[count] << " ";
cout << endl;


    return 0;
}
